package com.company;
import java.util.Scanner;
public class Main {

    public static long func(int my_savings){
        my_savings+=10000;
        my_savings*=2;
        my_savings+=30000;
        my_savings++;
        my_savings/=2;
        return my_savings;
    }

    public static void main(String[] args) {
       String my_name;
        System.out.print("Enter your name: ");
        Scanner str = new Scanner(System.in);
        my_name = str.nextLine();
       int my_savings=0;
       long MySavings= func(my_savings);
        System.out.println(my_name+ " has savings of: "+ MySavings);
        my_name="Bill Gates";
        my_savings=1000000;
        MySavings= func(my_savings);
        System.out.println(my_name+ " has savings of: "+ MySavings);
        }
    }
